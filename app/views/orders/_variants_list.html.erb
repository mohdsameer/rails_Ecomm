<% variants.each do |variant| %>
  <div class="scroll_content check_content">
    <div class="image_text_wrap">
      <%= check_box_tag 'variant_ids[]', variant.id, class: "check_box" %>1
      <% if variant.image.attached? %>
        <div class="img_box">
          <%= image_tag variant.image %>
        </div>
      <% else %>
        <div class="img_box">
          <%= image_tag asset_path('dummy-product.png') %>
        </div>
      <% end %>
      <div class="img_text_box">
        <div class="product_name"><%= variant.color %></div>
      </div>
    </div>
    <!-- <a href="<%#= all_producer_orders_path(variant: variant, product: @product, order: params[:order_id]) %>"
       data-turbo-frame="all-producer-order-modal"
       class="choose_pro_link"> -->

      <%#= image_tag "right_arrow", alt: "icon", class: "rigth_arrow" %>
    <!-- </a> -->
  </div>
<% end %>
